{
  "manifest_version": 3,
  "name": "SCU Schedule Helper",
  "version": "0.0.1",
  "description": "Next.js Chrome Extension for SCU Schedule Helper",
  "icons": {
    "16": "/icons/icon-16.png",
    "32": "/icons/icon-32.png",
    "48": "/icons/icon-48.png",
    "192": "/icons/icon-128.png"
  },
  "permissions": [],
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["inject.js"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html"
  }
}
